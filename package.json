{
  "name": "react-beyond",
  "version": "0.0.1",
  "description": "Is modern bolilerplate for React, includes Webpack, Redux, React-Router, SSR, SASS",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "dev": "cross-env NODE_ENV=development nodemon ./src/server/server.babel.js --watch src/server/server.js & nodemon ./src/server/api.babel.js --watch src/server/api.js src/server/service --no-warnings",
    "build": "npm run build:client && npm run build:server",
    "build:server": "./node_modules/.bin/webpack --config ./webpack/webpack.config.server.js --progress",
    "build:client": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack/webpack.config.client.js --progress"
  },
  "keywords": [
    "react",
    "webpack",
    "hmr",
    "ssr",
    "redux",
    "express",
    "bulma",
    "styled",
    "boilerplate",
    "router"
  ],
  "author": "Pavel Torbeev",
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-system-import-transformer": "^3.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "node-sass": "^4.5.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.18.2",
    "bulma": "^0.5.3",
    "cors": "^2.8.4",
    "express": "^4.15.5",
    "lodash": "^4.17.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "styled-components": "^2.2.0"
  }
}
